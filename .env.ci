# CI Environment Configuration
CI=true
# SECURITY: Replace with actual secret key - use GitHub Secrets in production CI/CD
SECRET_KEY=your-secret-key-here
VANA_PORT=8000
VANA_HOST=127.0.0.1
GOOGLE_CLOUD_PROJECT=analystai-454200
AUTH_REQUIRE_SSE_AUTH=false
USE_OPENROUTER=false
# Environment for security middleware
NODE_ENV=production
# CORS Origins - secure defaults
ALLOW_ORIGINS=https://vana.yourdomain.com
# CSP Extra Connect Domains
CSP_EXTRA_CONNECT_DOMAINS=https://api.openai.com,https://api.anthropic.com
# Skip cloud services in CI
SKIP_CLOUD_LOGGING=true
SKIP_GCS_BUCKET=true
SKIP_TRACING=true
# Security scanning settings
SECURITY_SCAN_ENABLED=true
SECURITY_FAIL_ON_CRITICAL=true