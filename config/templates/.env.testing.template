# VANA ADK Memory Testing Configuration Template
# Copy this file to .env and update values as needed

# Google Cloud Configuration (REQUIRED)
GOOGLE_CLOUD_PROJECT=analystai-454200
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=/path/to/vana-service-account-key.json
GOOGLE_GENAI_USE_VERTEXAI=True

# ADK Memory Configuration (TESTING OPTIMIZED)
ADK_RAG_CORPUS=projects/analystai-454200/locations/us-central1/ragCorpora/vana-corpus-test
ADK_SIMILARITY_TOP_K=2
ADK_VECTOR_DISTANCE_THRESHOLD=0.5
ADK_SESSION_SERVICE_TYPE=memory

# Environment Configuration
VANA_ENV=testing

# Vector Search Configuration (OPTIONAL - Preserved from legacy)
VECTOR_SEARCH_ENDPOINT_ID=your-test-endpoint-id
DEPLOYED_INDEX_ID=vanasharedindex-test

# Performance Configuration (TESTING OPTIMIZED)
ADK_MEMORY_CACHE_SIZE=100
ADK_MEMORY_CACHE_TTL=300
ADK_MAX_CONCURRENT_REQUESTS=2
ADK_REQUEST_TIMEOUT=10
ADK_RETRY_ATTEMPTS=1
ADK_BACKOFF_FACTOR=1.0

# Logging Configuration (TESTING)
LOG_LEVEL=DEBUG
LOG_FORMAT=text
LOG_FILE=./logs/vana-test.log

# Security Configuration (TESTING)
ADK_ENABLE_TLS=false
ADK_TLS_VERIFY=false

# Monitoring Configuration (TESTING)
ADK_METRICS_ENABLED=false
ADK_CONFIG_MONITORING=false
ADK_HEALTH_CHECK_INTERVAL=30

# Multi-Agent Configuration (TESTING)
VANA_AGENT_COUNT=2
VANA_TOOL_COUNT=10
VANA_ENABLE_PERFORMANCE_MONITORING=false
VANA_DEBUG_MODE=true
VANA_TEST_MODE=true

# Testing-specific Configuration
TEST_DATA_DIR=./tests/data
TEST_FIXTURES_DIR=./tests/fixtures
MOCK_EXTERNAL_SERVICES=true
ENABLE_TEST_LOGGING=true
