{
  "name": "vana-project",
  "version": "2.0.0",
  "description": "Vana AI Platform - Unified Frontend and Backend",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:dev": "cd app && python -m uvicorn server:app --reload --port 8000",
    "frontend:build": "cd frontend && npm run build",
    "frontend:start": "cd frontend && npm start",
    "backend:start": "cd app && python -m uvicorn server:app --port 8000",
    "start": "concurrently \"npm run backend:start\" \"npm run frontend:start\"",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd app && pip install -r requirements.txt",
    "install:all": "npm install && npm run install:frontend",
    "test": "export PLAYWRIGHT=True...",
    "lint": "cd frontend && npm run lint",
    "clean": "rm -rf node_modules frontend/node_modules frontend/.next"
  },
  "keywords": [
    "ai",
    "chat",
    "vana",
    "nextjs",
    "fastapi"
  ],
  "author": "Vana Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.5",
    "eventsource": "^4.0.0",
    "playwright": "^1.55.1"
  }
}
