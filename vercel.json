{
  "framework": "nextjs",
  "rootDirectory": "frontend",
  "installCommand": "npm ci",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://vana-staging-analystai-454200.a.run.app/api/:path*"
    },
    {
      "source": "/agent_network_sse/:path*",
      "destination": "https://vana-staging-analystai-454200.a.run.app/agent_network_sse/:path*"
    },
    {
      "source": "/agent_network_events/:path*",
      "destination": "https://vana-staging-analystai-454200.a.run.app/agent_network_events/:path*"
    },
    {
      "source": "/health",
      "destination": "https://vana-staging-analystai-454200.a.run.app/health"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@api_url",
    "NEXT_PUBLIC_ENVIRONMENT": "@environment"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "@api_url",
      "NEXT_PUBLIC_ENVIRONMENT": "@environment",
      "NODE_ENV": "production"
    }
  }
}