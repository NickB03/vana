# Example environment configuration
VANA_MODEL=your-model
GOOGLE_GENAI_USE_VERTEXAI=False
GOOGLE_API_KEY=your-google-api-key
OPENROUTER_API_KEY=your-openrouter-api-key
VANA_ENV=development
VANA_HOST=localhost
VANA_PORT=8081
LOG_LEVEL=DEBUG
VANA_RAG_CORPUS_ID=your-corpus-id
RAG_CORPUS_RESOURCE_NAME=your-resource-name
GOOGLE_CLOUD_PROJECT=your-project
GOOGLE_CLOUD_LOCATION=us-central1
MEMORY_SIMILARITY_TOP_K=5
MEMORY_VECTOR_DISTANCE_THRESHOLD=0.7
SESSION_SERVICE_TYPE=in_memory
# Google Custom Search Engine ID (optional - uses public CSE if not set)
GOOGLE_CSE_ID=your-custom-search-engine-id
DATABASE_URL=sqlite:///./sessions.db
DASHBOARD_ENABLED=true
USE_LOCAL_MCP=true
VANA_USE_MOCK=false

# Agentic AI Configuration
VANA_AGENT_MODULE=agents.vana.team_agentic  # Use hierarchical agent system
VANA_AGENT_PHASE=1  # Current implementation phase
VANA_ENABLE_SPECIALISTS=true  # Enable specialist agents
VANA_MAX_TOOLS_PER_AGENT=6  # ADK best practice limit

# ADK Event Streaming (Production Ready)
USE_ADK_EVENTS=true  # Enable silent agent handoffs and real-time event streaming
