# Basic Authentication
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=your_username
N8N_BASIC_AUTH_PASSWORD=your_password

# Webhook URL (Railway will provide this after deployment)
WEBHOOK_URL=https://your-n8n-instance.railway.app

# Ragie API Key
RAGIE_API_KEY=your_ragie_api_key

# Database Configuration (Railway will provide these)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=railway_provided_host
DB_POSTGRESDB_PORT=railway_provided_port
DB_POSTGRESDB_DATABASE=railway_provided_database
DB_POSTGRESDB_USER=railway_provided_user
DB_POSTGRESDB_PASSWORD=railway_provided_password

# Additional Configuration
N8N_PORT=$PORT
N8N_PROTOCOL=https
NODE_ENV=production
GENERIC_TIMEZONE=UTC
