{
  "mcpServers": {
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"],
      "env": {
        "PUPPETEER_HEADLESS": "new",
        "PUPPETEER_ARGS": "--no-sandbox,--disable-setuid-sandbox",
        "PUPPETEER_TIMEOUT": "30000"
      }
    }
  }
}
